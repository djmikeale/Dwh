                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH InputColumns AS
(
SELECT 
	N'SourceTable' AS SchemaName,
	TABLE_NAME AS TableName,
	COLUMN_NAME AS ColumnName, 
	CASE DATA_TYPE
		WHEN 'nchar' THEN DATA_TYPE + '(' + CAST(CHARACTER_MAXIMUM_LENGTH AS nvarchar) + ')'
		WHEN 'nvarchar' THEN DATA_TYPE + '(' + REPLACE(CAST(CHARACTER_MAXIMUM_LENGTH AS nvarchar),'-1','max') + ')'
		WHEN 'decimal' THEN DATA_TYPE + '(' + CAST(NUMERIC_PRECISION AS nvarchar) + ',' + CAST(NUMERIC_SCALE AS nvarchar) + ')'
		WHEN 'datetime2' THEN DATA_TYPE + '(' + CAST(DATETIME_PRECISION AS nvarchar) + ')'
		ELSE DATA_TYPE /* Surely this will fail at some point but I'll fix that when it breaks lol -- SELECT * FROM sys.types could be useful */ 
		END AS 'DataType',
	CASE WHEN IS_NULLABLE = 'YES' THEN N'NULL' ELSE N'NOT NULL' END AS IsNullable,
	N'' AS ColProperty
FROM INFORMATION_SCHEMA.COLUMNS
)
SELECT *
     FROM InputColumns</property>
