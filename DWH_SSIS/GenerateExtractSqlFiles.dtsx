    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{8607D895-3C92-4078-8B85-3C26134BB352}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;WITH CTE AS &#xA;(&#xA;SELECT &#xA;      'Extract' AS DstSchema&#xA;&#x9;  ,[TableName]&#xA;      , (SELECT '[' + ColumnName + '] ' + UPPER(DataType) + ' ' + IsNullable + CASE WHEN ColProperty = '' THEN ', ' ELSE ' ' + ColProperty + ', ' END&#xA;        FROM [DWH].[DWHColumns] AS innerTbl&#xA;        WHERE innerTbl.[TableName] = tbl.[TableName] OR TableName = ''&#xA;        FOR XML PATH(''))&#xA;    AS cols&#xA;  FROM [DWH].[DWHColumns] AS Tbl&#xA;  WHERE TableName = '&quot; + @[User::Temp_TableName] + &quot;'&#xA;  GROUP BY TableName&#xA;&#xA;&#xA;)&#xA;&#xA;SELECT CAST('CREATE TABLE [Extract].[' + TableName + + '] (' + cols + 'CONSTRAINT [PK_' + DstSchema + '_' + TableName + '] PRIMARY KEY CLUSTERED ([DWH_ID] ASC))' AS nvarchar(max)) AS QueryContent FROM CTE&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="TableDDL">
      <DTS:VariableValue
        DTS:DataType="8">WITH CTE AS 
(
SELECT 
      'Extract' AS DstSchema
	  ,[TableName]
      , (SELECT '[' + ColumnName + '] ' + UPPER(DataType) + ' ' + IsNullable + CASE WHEN ColProperty = '' THEN ', ' ELSE ' ' + ColProperty + ', ' END
        FROM [DWH].[DWHColumns] AS innerTbl
        WHERE innerTbl.[TableName] = tbl.[TableName] OR TableName = ''
        FOR XML PATH(''))
    AS cols
  FROM [DWH].[DWHColumns] AS Tbl
  WHERE TableName = 'Address'
  GROUP BY TableName


)

SELECT CAST('CREATE TABLE [Extract].[' + TableName + + '] (' + cols + 'CONSTRAINT [PK_' + DstSchema + '_' + TableName + '] PRIMARY KEY CLUSTERED ([DWH_ID] ASC))' AS nvarchar(max)) AS QueryContent FROM CTE</DTS:VariableValue>
    </DTS:Variable>
